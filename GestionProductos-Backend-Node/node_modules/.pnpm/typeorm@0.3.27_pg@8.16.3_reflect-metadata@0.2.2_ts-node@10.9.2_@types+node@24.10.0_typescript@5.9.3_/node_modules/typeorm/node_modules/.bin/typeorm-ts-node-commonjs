#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/camilo/Documentos/PruebaTecnicaIgloolab/GestionProductos-Backend-Node/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.10.0_typescript@5.9.3_/node_modules/typeorm/node_modules:/home/camilo/Documentos/PruebaTecnicaIgloolab/GestionProductos-Backend-Node/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.10.0_typescript@5.9.3_/node_modules:/home/camilo/Documentos/PruebaTecnicaIgloolab/GestionProductos-Backend-Node/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/camilo/Documentos/PruebaTecnicaIgloolab/GestionProductos-Backend-Node/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.10.0_typescript@5.9.3_/node_modules/typeorm/node_modules:/home/camilo/Documentos/PruebaTecnicaIgloolab/GestionProductos-Backend-Node/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.10.0_typescript@5.9.3_/node_modules:/home/camilo/Documentos/PruebaTecnicaIgloolab/GestionProductos-Backend-Node/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-commonjs.js" "$@"
fi
